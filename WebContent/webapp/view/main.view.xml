<mvc:View controllerName="com.tcs.ShoppingApp.controller.main" 
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	displayBlock="true" xmlns="sap.m">
	<tnt:ToolHeader>
		<Button text="Account" type="Transparent" press="navToAccounts">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low"/>
			</layoutData>
		</Button>
		<Button text="Shop" type="Transparent" press="navToShop">
			<layoutData>
				<OverflowToolbarLayoutData priority="Low" />
			</layoutData>
		</Button>
	</tnt:ToolHeader>
	<SplitApp id="rootControl" />
</mvc:View>